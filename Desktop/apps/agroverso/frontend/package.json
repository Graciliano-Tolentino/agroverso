    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.3.2",
    "snyk": "^1.1285.0",
    "tailwindcss": "^4.1.6",
    "terser": "^5.30.4",
